mount -o remount,rw /system
echo '1228800' > /system/etc/andrev_oc/wake_max
echo '1228800' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
echo 'Tweaked' > /system/media/uvscripts/profile
rm -f /system/etc/init.d/84fj
echo 'echo '1228800' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq' > /system/etc/init.d/84fjwmax
chmod 755 /system/etc/init.d/84fjwmax
