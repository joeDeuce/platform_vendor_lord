absorecovery() {
FILE=abso4ext.tgz
MD5SUM=6d4c1724e43e5a33e5afe39c6f9a8dcc
URL="http://rootdev.org/Redemption/updates/abso4ext.tgz"
if [ ! -d /sdcard/download/rectmp ]; then
  MK=1
  mkdir -p /sdcard/download/rectmp
fi
cd /sdcard/download/rectmp
if [ -f "$FILE" ]; then
MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then 
echo "Downloading Package Be Patient"
rm  "$FILE"
wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
  mount -o remount,rw /system
  cd /sdcard/download/rectmp
echo "Extracting Package"
  tar -xzf abso4ext.tgz
echo "Flashing Recovery"
  cp recovery.img /sdcard/recovery.img
flash_image recovery /sdcard/recovery.img
  cd /sdcard/download/rectmp
echo "Copying Data"
  cp -R /sdcard/download/rectmp/cwtheme/4ext /system
echo "Cleaning up"
    mount -o remount,ro /system
  cd - >/dev/null 2>&1
  echo "Done! Please Reboot and Enjoy Redemption Touch Recovery"
else
  echo "Failed to Install Redemption Touch Recovery"
  echo "Try Again"
  exit 1
fi
rm  /sdcard/recovery.img
rm  /sdcard/download/rectmp/$FILE
rm -R /sdcard/download/rectmp

if [ "$MK" = "1" ]; then
  rm -R /sdcard/download/rectmp 
fi
}

agguvon() {
mount -o remount,rw /system
rm  /system/etc/init.d/99voltages
rm  /system/etc/init.d/01kstune
cp /system/media/uvscripts/75uv/99voltages /system/etc/init.d/99voltages
chmod 777 /system/etc/init.d/99voltages
/system/etc/init.d/99voltages
echo 'Redemption Aggressive Undervolting -75mV' > /system/media/uvscripts/undervolt
mount -o remount,ro /system
echo Activated Redemption Aggressive Undervolting -75mV!
}

callrecorder() {
FILE=CallRecorder_v.1.0.27_alpha_build05.apk
MD5SUM=711b5324d5bbe85667bf4ea4beb87bf6
URL="http://rootdev.org/Redemption/updates/CallRecorder_v.1.0.27_alpha_build05.apk"
if [ ! -d /sdcard/tmp ]; then
  MK=1
  mkdir -p /sdcard/tmp
fi
cd /sdcard/tmp
if [ -f "$FILE" ]; then
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then
  rm  "$FILE"
  wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
  mount -o remount,rw /system
  cd /
  rm  /data/app/*callrecorder*
  rm  /data/app/*CallRecorder*
  cp /sdcard/tmp/CallRecorder_v.1.0.27_alpha_build05.apk /data/app/CallRecorder_v.1.0.27_alpha_build05.apk
  chmod 644 /data/app/CallRecorder_v.1.0.27_alpha_build05.apk
  mount -o remount,ro /system
  cd - >/dev/null 2>&1
  echo "Done! Enjoy Recording Calls!"
else
  echo "Failed to download / install Call Recorder"
  exit 1
fi
rm  /sdcard/tmp/$FILE

if [ "$MK" = "1" ]; then
  rmdir /sdcard/tmp 
fi
}

