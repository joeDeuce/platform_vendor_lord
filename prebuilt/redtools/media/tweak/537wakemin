mount -o remount,rw /system
echo '537600' > /system/etc/andrev_oc/wake_min
echo '537600' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
echo 'Tweaked' > /system/media/uvscripts/profile
rm -f /system/etc/init.d/84fj
echo 'echo '537600' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq' > /system/etc/init.d/84fjwmin
chmod 755 /system/etc/init.d/84fjwmin
