enablebootsound() {
mount -o remount,rw /system
mkdir /sdcard/bootsound
#This is to create the needed scripts in init.local.rc
echo '' >> /system/etc/init.local.rc
echo '' >> /system/etc/init.local.rc
echo '###Bootsound - Safe to Delete: Start ###' >> /system/etc/init.local.rc
echo "" >> /system/etc/init.local.rc
echo 'service bootsound /system/bin/bootsound' >> /system/etc/init.local.rc
echo '        user media' >> /system/etc/init.local.rc
echo '        group audio' >> /system/etc/init.local.rc
echo '        disabled' >> /system/etc/init.local.rc
echo '        oneshot' >> /system/etc/init.local.rc
echo '' >> /system/etc/init.local.rc
echo 'on property:init.svc.bootanim=running   # to correct timin' >> /system/etc/init.local.rc
echo '        start bootsound' >> /system/etc/init.local.rc
echo '' >> /system/etc/init.local.rc
echo 'on property:dev.bootcomplete=1' >> /system/etc/init.local.rc
echo '        stop bootsound' >> /system/etc/init.local.rc
echo '### Bootsound - Safe to Delete: End ###' >> /system/etc/init.local.rc
#Now creating the bootsound script
touch /system/bin/bootsound
echo '#!/system/bin/sh' >> /system/bin/bootsound
echo '' >> /system/bin/bootsound
echo 'bprop=/system/build.prop' >> /system/bin/bootsound
echo '' >> /system/bin/bootsound
echo 'play=$(cat $bprop | egrep -c -i -o ro.config.play.bootsound=1)' >> /system/bin/bootsound
echo 'if [ "$play" = "1" ]; then' >> /system/bin/bootsound
echo '        stagefright -a -o /sdcard/bootsound/*.mp3' >> /system/bin/bootsound
echo 'fi' >> /system/bin/bootsound
echo 'exit 0' >> /system/bin/bootsound
#Changing some permissions
chmod 777 /system/bin/bootsound
chown root:shell /system/bin/bootsound
echo '' >> /system/build.prop
echo 'ro.config.play.bootsound=1' >> /system/build.prop
mount -o remount,ro /system
echo Boot Sound enabled!
echo Place any mp3  in /sdcard/bootsound folder
echo Be sure to have ONLY One mp3 in this folder!
mount -o remount,ro /system
}

ondemandsleep() {
mount -o remount,rw /system
echo 'ondemand' > /system/etc/andrev_oc/sleep_gov
mount -o remount,ro /system
echo Sleep Gov ondemand!
}

#### Main execution
main() {
  CMD=$1
  shift

  #### Recovery mode
  if [ "$RECOVERY_MODE" = "1" ]; then
    ALLOWED_CMD=`echo $RECOVERY_CMD_WHITELIST | grep $CMD`
    if [ "$ALLOWED_CMD" = "" ]; then
      echo "Command $CMD not allowed in recovery mode"
      exit 1
    fi
  fi

  #### All of supported commands
  case $CMD in

    "190dpi") 190dpi ;;   # Hardcoded into ABTOOL.apk
    "192dpi") 192dpi ;;   # *
    "200dpi") 200dpi ;;   # *
    "210dpi") 210dpi ;;   # *
    "220dpi") 220dpi ;;   # Hardcoded into ABTOOL.apk
    "240dpi") 240dpi ;;   # *
    "absorecovery") absorecovery ;;   # *
    "agguvon") agguvon ;;   # *
    "callrecorder") callrecorder ;;   # Hardcoded into ABTOOL.apk
    "collrecovery") collrecovery ;;   # *
    "desirehd") desirehd ;;   # *
    "dspmanager") dspmanager ;;   # *
    "flappjaxxx") flappjaxxx ;;   # Hardcoded into ABTOOL.apk
    "gallery3d") gallery3d ;;   # *
    "googleapps") googleapps ;;   # *
    "gpsatt") gpsatt ;;   # *
    "gpsgeneric") gpsgeneric ;;   # *
    "gpsstock") gpsstock ;;   # *
    "gpstmobile") gpstmobile ;;   # *
    "gpsww") gpsww ;;   # *
    "gpswwa") gpswwa ;;   # *
    "inspire4g") inspire4g ;;   # *
    "liteuvon") liteuvon ;;   # *
    "lonestar") lonestar ;;   # *
    "miuicam17") miuicam17 ;;   # *
    "noadblock") noadblock ;;   # *
    "nunhug") nunhug ;;   # *
    "ocuvwhat") ocuvwhat ;;   # *
    "rasroy") rasroy ;;   # *
    "rmcustprof") rmcustprof ;;   # *
    "rmsndmod") rmsndmod ;;   # *
    "rmsupl") rmsupl ;;   # *
    "sloth") sloth ;;   # *
    "sndmod") sndmod ;;   # *
    "stockcam") stockcam ;;   # *
    "stuvon") stuvon ;;   # *
    "toolschangelog") toolschangelog ;;   # *
    "uvoff") uvoff ;;   # *
    "volumeplus") volumeplus ;;   # *
    "yesadblock") yesadblock ;;   # *
    "startota") startota ;;   # *
    "startfjt") startfjt ;;   # *
    "upabtools") upabtools ;;   # *
    "apexlauncher") apexlauncher ;;   # *
    "novalauncher") novalauncher ;;   # *
    "treblauncher") treblauncher ;;   # *
    "absocam") absocam ;;   # *
    "dhdsound") dhdsound ;;   # *
    "inspiresound") inspiresound ;;   # *
    "apnstock") apnstock ;;   # *
    "apnalts") apnalts ;;   # *
    "buildpfix") buildpfix ;;   # *
    "buildprevert") buildprevert ;;   # *
    "launcher2") launcher2 ;;   # *
    "enablebootsound") enablebootsound ;;   # *
    "disablebootsound") disablebootsound ;;   # *

  
    ### Any other command - we are a wrapper
    *)            exec $CMD $@ ;;
    esac

}

## Normal execution - just main
main $@
exit $?
