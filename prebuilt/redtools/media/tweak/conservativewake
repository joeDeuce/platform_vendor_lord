mount -o remount,rw /system
echo 'conservative' > /system/etc/andrev_oc/wake_gov
echo 'conservative' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor
rm -f /system/etc/init.d/84fj
echo 'echo 'conservative' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor' > /system/etc/init.d/84fjwgov
chmod 755 /system/etc/init.d/84fjwgov
