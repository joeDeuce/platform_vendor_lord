buildpfix() {
mount -o remount,rw /system
/system/xbin/sed -i 's/ro.ril.hsdpa.category=10/ro.ril.hsdpa.category=28/g' /system/build.prop
/system/xbin/sed -i 's/ro.ril.hsxpa=2/ro.ril.hsxpa=3/g' /system/build.prop
mount -o remount,ro /system
echo Build.prop Data Tweaked, Reboot to Activate!
}

buildprevert() {
mount -o remount,rw /system
/system/xbin/sed -i 's/ro.ril.hsdpa.category=28/ro.ril.hsdpa.category=10/g' /system/build.prop
/system/xbin/sed -i 's/ro.ril.hsxpa=3/ro.ril.hsxpa=2/g' /system/build.prop
mount -o remount,ro /system
echo Build.prop Data Fix Reverted, Reboot to Activate!
}

